{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@_types/*": [
        "./src/_types/*"
      ],
      "@helpers/*": [
        "./src/_helpers/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@data/*": [
        "./src/_data/*"
      ],
      "@xpack/*": [
        "./xpack/*"
      ],
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.json"
  ],
  "extends": "../../tsconfig.base.json"
}